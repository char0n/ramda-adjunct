{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": [true, "never-prefix"],
      "ban-types": [true, ["Object", "Use {} instead."], ["String"], ["Number"]],
      "member-ordering": false,
      "no-namespace": false
    },
    "rulesDirectory": []
}
